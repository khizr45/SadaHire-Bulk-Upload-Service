{
  "name": "sadahire-cv-microservice",
  "version": "1.0.0",
  "description": "SadaHire CV processing microservice (Redis + BullMQ + S3 support)",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "concurrently \"node dist/server.js\" \"node dist/worker.js\"",
    "dev:server": "ts-node src/server.ts",
    "dev:worker": "ts-node src/worker.ts",
    "start:prod-server": "node dist/server.js",
    "start:prod-worker": "node dist/worker.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.637.0",
    "cors": "^2.8.5",
    "axios": "^1.5.0",
    "bullmq": "^1.90.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.0.0",
    "express": "^5.1.0",
    "form-data": "^4.0.0",
    "ioredis": "^5.3.2",
    "multer": "^1.4.5-lts.1",
    "stream-to-array": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
